{
  "name": "meditation-app",
  "version": "1.0.0",
  "scripts": {
    "start-blitz": "blitz db migrate && blitz start",
    "start": "yarn start-blitz",
    "start:docker": "docker-compose up -d && yarn start-blitz",
    "studio": "blitz db studio",
    "build": "NODE_ENV=production blitz db migrate && blitz build",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch",
    "pre-commit": "tsc && lint-staged && pretty-quick --staged",
    "pre-push": "npm run lint && npm run test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "browserslist": [
    "defaults"
  ],
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "printWidth": 100
  },
  "husky": {
    "hooks": {}
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "@prisma/cli": "2.x",
    "@prisma/client": "2.x",
    "@sendgrid/mail": "7.3.0",
    "blitz": "0.24.3",
    "final-form": "4.20.1",
    "react": "0.0.0-experimental-7f28234f8",
    "react-bootstrap": "1.3.0",
    "react-dom": "0.0.0-experimental-7f28234f8",
    "react-dropzone": "11.2.0",
    "react-error-boundary": "2.3.2",
    "react-final-form": "6.x",
    "sass": "1.27.0",
    "secure-password": "4.x",
    "zod": "1.x"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@storybook/addon-actions": "^6.0.27",
    "@storybook/addon-essentials": "^6.0.27",
    "@storybook/addon-links": "^6.0.27",
    "@storybook/react": "^6.0.27",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.x",
    "@testing-library/react-hooks": "3.x",
    "@types/chance": "1.1.0",
    "@types/jest": "26.0.14",
    "@types/react": "16.x",
    "@types/secure-password": "3.x",
    "@typescript-eslint/eslint-plugin": "4.x",
    "@typescript-eslint/parser": "4.x",
    "babel-eslint": "10.1.0",
    "babel-loader": "^8.1.0",
    "chance": "1.1.7",
    "eslint": "7.x",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-flowtype": "5.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.21.4",
    "eslint-plugin-react-hooks": "4.x",
    "husky": "4.3.0",
    "jest": "26.x",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-watch-typeahead": "0.x",
    "lint-staged": "10.x",
    "prettier": "2.x",
    "pretty-quick": "3.1.0",
    "react-is": "^17.0.1",
    "react-test-renderer": "16.14.0",
    "ts-jest": "26.x",
    "typescript": "4.x"
  },
  "private": true
}
